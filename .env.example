# Copy this file to .env and fill in your values.

# Required: API key for Limitless Developer API
LIMITLESS_API_KEY=

# Optional: override API URL (default: https://api.limitless.ai)
# LIMITLESS_API_URL=

# Optional: default timezone (IANA name, e.g. America/Los_Angeles)
# LIMITLESS_TZ=

# Optional: data root, defaults to ~/limitless_tools/data/lifelogs
# LIMITLESS_DATA_DIR=

